			<attr>http://egee.cesnet.cz/en/Schema/JP/System:owner</attr>
			<op>EQUAL</op>
			<value>
				<string>/DC=cz/DC=cesnet-ca/O=Masaryk University/CN=Milos Mulac</string>
			</value>
		</condition>
		<history>1</history>
