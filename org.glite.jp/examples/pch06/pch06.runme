#!/bin/sh

if [ -z "$6" ]; then
	echo usage: $0 reference base1 base2 base3 base4 atlas
	echo "	all args are URLs without suffix"
	exit 1
fi

sed "s/REFERENCE/$1/; s/BASE1/$2/; s/BASE2/$3/; s/BASE3/$4/; s/BASE4/$5/; s/ATLAS/$6/" pch06.jdl-template >pch06.jdl

